{
  "version": 2,
  "name": "jajapo",
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "regions": ["gru1"],
  "functions": {
    "app.js": {
      "includeFiles": "views/**"
    }
  }
}
